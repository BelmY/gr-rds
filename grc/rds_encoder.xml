<?xml version="1.0"?>
<block>
	<name>RDS Encoder</name>
	<key>gr_rds_encoder</key>
	<category>RDS</category>
	<import>import rds</import>
	<make>rds.encoder($pty_locale, $ms, $ps)</make>
	<param>
		<name>PTY Locale</name>
		<key>pty_locale</key>
		<value>0</value>
		<type>int</type>
		<option>
			<name>Europe</name>
			<key>0</key>
		</option>
		<option>
			<name>North America</name>
			<key>1</key>
		</option>
	</param>
	<param>
		<name>MS</name>
		<key>ms</key>
		<value>True</value>
		<type>bool</type>
        <option>
            <name>Music</name>
            <key>True</key>
        </option>
        <option>
            <name>Speech</name>
            <key>False</key>
        </option>
	</param>
	<param>
		<name>PS</name>
		<!-- <description>Program Service Name</description> -->
		<key>ps</key>
		<value>WDR 3</value>
		<type>string</type>
	</param>
	<sink>
		<name>rds in</name>
		<type>message</type>
		<optional>1</optional>
	</sink>
	<source>
		<name>out</name>
		<type>byte</type>
	</source>
    <doc>
MS: Speech/Music
PS: Program Service Name
    </doc>
</block>
